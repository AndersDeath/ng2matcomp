{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"project": {
		"name": "ng2matcomp"
	},
	"apps": [
		{
			"root": "src",
			"outDir": "dist",
			"assets": [
				"assets",
				"favicon.ico",
				"../node_modules/material-design-icon-fonts/iconfont/MaterialIcons-Regular.eot",
				"../node_modules/material-design-icon-fonts/iconfont/MaterialIcons-Regular.svg",
				"../node_modules/material-design-icon-fonts/iconfont/MaterialIcons-Regular.ttf",
				"../node_modules/material-design-icon-fonts/iconfont/MaterialIcons-Regular.woff",
				"../node_modules/material-design-icon-fonts/iconfont/MaterialIcons-Regular.woff2"
			],
			"index": "index.html",
			"main": "main.ts",
			"polyfills": "polyfills.ts",
			"test": "test.ts",
			"tsconfig": "tsconfig.app.json",
			"testTsconfig": "tsconfig.spec.json",
			"prefix": "app",
			"styles": [
				"../node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
				"../node_modules/material-design-icon-fonts/iconfont/material-icons.css",
				"../node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
				"styles.css"
			],
			"scripts": [
				"../node_modules/web-animations-js/web-animations.min.js"
			],
			"environmentSource": "environments/environment.ts",
			"environments": {
				"dev": "environments/environment.ts",
				"prod": "environments/environment.prod.ts"
			}
		}
	],
	"e2e": {
		"protractor": {
			"config": "./protractor.conf.js"
		}
	},
	"lint": [
		{
			"project": "src/tsconfig.app.json",
			"exclude": "**/node_modules/**"
		},
		{
			"project": "src/tsconfig.spec.json",
			"exclude": "**/node_modules/**"
		},
		{
			"project": "e2e/tsconfig.e2e.json",
			"exclude": "**/node_modules/**"
		}
	],
	"packageManager": "npm",
	"test": {
		"karma": {
			"config": "./karma.conf.js"
		}
	},
	"defaults": {
		"styleExt": "css",
		"component": {}
	}
}
